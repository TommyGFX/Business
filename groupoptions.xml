<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com/XSD/group-options.xsd">
	<import>
		<categories>
			<category name="user.business">
				<parent>user</parent>
				<options>module_business</options>
			</category>
			<category name="user.business.links">
				<parent>user.business</parent>
			</category>
			<category name="user.business.attachments">
				<parent>user.business</parent>
			</category>
			<category name="user.business.comments">
				<parent>user.business</parent>
			</category>
			
			<category name="mod.business">
				<parent>mod</parent>
				<options>module_business</options>
			</category>
			<category name="mod.business.links">
				<parent>mod.business</parent>
			</category>
			<category name="mod.business.comments">
				<parent>mod.business</parent>
			</category>
			
			<category name="admin.business">
				<parent>admin.content</parent>
			</category>
		</categories>
		
		<options>
			<!-- user.business.links -->
			<option name="user.business.canViewBusiness">
				<categoryname>user.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.business.canViewCategory">
				<categoryname>user.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.business.canEnterCategory">
				<categoryname>user.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.business.canViewLink">
				<categoryname>user.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.business.canEnterLink">
				<categoryname>user.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.business.canAddLink">
				<categoryname>user.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>user.business.canAddLinkWithoutModeration</enableoptions>
			</option>
			<option name="user.business.canAddLinkWithoutModeration">
				<categoryname>user.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="user.business.canEditOwnLink">
				<categoryname>user.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="user.business.canVisitLink">
				<categoryname>user.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.business.canRateLink">
				<categoryname>user.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.business.canReportLink">
				<categoryname>user.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<!-- /user.business.links -->
			
			<!-- user.business.attachments -->
			<option name="user.business.canDownloadAttachment">
				<categoryname>user.business.attachments</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.business.canViewAttachmentPreview">
				<categoryname>user.business.attachments</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.business.canUploadAttachment">
				<categoryname>user.business.attachments</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>user.business.maxAttachmentSize,user.business.allowedAttachmentExtensions,user.business.maxAttachmentCount</enableoptions>
			</option>
			<option name="user.business.maxAttachmentSize">
				<categoryname>user.business.attachments</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>300000</defaultvalue>
			</option>
			<option name="user.business.allowedAttachmentExtensions">
				<categoryname>user.business.attachments</categoryname>
				<optiontype>textarea</optiontype>
				<defaultvalue><![CDATA[gif
jpg
jpeg
png
gif
bmp
zip
txt
pdf]]></defaultvalue>
			</option>
			<option name="user.business.maxAttachmentCount">
				<categoryname>user.business.attachments</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>5</defaultvalue>
			</option>
			<!-- /user.business.attachments -->

			<!-- user.business.comments -->
			<option name="user.business.canSeeComments">
				<categoryname>user.business.comments</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.business.canAddComment">
				<categoryname>user.business.comments</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>user.business.maxCommentLength</enableoptions>
			</option>
			<option name="user.business.maxCommentLength">
				<categoryname>user.business.comments</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>1000</defaultvalue>
			</option>
			<option name="user.business.canEditOwnComment">
				<categoryname>user.business.comments</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<!-- /user.business.comments -->
			
			
			
			<!-- mod.business.links -->
			<option name="mod.business.canEnableLinks">
				<categoryname>mod.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="mod.business.canEditLinks">
				<categoryname>mod.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="mod.business.canDeleteLinks">
				<categoryname>mod.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="mod.business.canSeeModerationOverview">
				<categoryname>mod.business.links</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<!-- /user.business.comments -->
			
			<!-- mod.business.comments -->
			<option name="mod.business.canEditComments">
				<categoryname>mod.business.comments</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="mod.business.canDeleteComments">
				<categoryname>mod.business.comments</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<!-- /mod.business.comments -->
			
			<!-- admin.business -->
			<option name="admin.business.canAddCategory">
				<categoryname>admin.business</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="admin.business.canEditCategory">
				<categoryname>admin.business</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<!-- /admin.business -->
			
		</options>
	</import>
</data>
